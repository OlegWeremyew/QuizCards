{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": "src",
    "paths": {
      "api/*": ["./api/*"],
      "assets/*": ["./assets/*"],
      "constants/*": ["./constants/*"],
      "features/*": ["./features/*"],
      "Redux/*": ["./Redux/*"],
      "selectors/*": ["./selectors/*"],
      "tests/*": ["./tests/*"],
      "types/*": ["./types/*"],
      "ui/*": ["./ui/*"],
      "utils/*": ["./utils/*"],
      },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}